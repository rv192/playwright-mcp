{
  "name": "@anthropic/mcp-server-camoufox",
  "version": "0.1.0",
  "description": "Playwright MCP server powered by Camoufox anti-detect browser",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rv192/playwright-mcp.git"
  },
  "homepage": "https://github.com/rv192/playwright-mcp",
  "engines": {
    "node": ">=20"
  },
  "author": {
    "name": "rv192"
  },
  "license": "Apache-2.0",
  "scripts": {
